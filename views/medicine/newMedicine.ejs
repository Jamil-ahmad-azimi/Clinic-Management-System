<body data-entity="medicine">
<h3>Add a New Medicine</h3>
<form id="addForm">
  <div class="row">
    <div class="col-md-4 mb-3">
      <label for="medicineName" class="form-label">Name</label>
      <input type="text" class="form-control" id="medicineName" placeholder="Enter medicine name" required>
    </div>
    <div class="col-md-4 mb-3">
      <label for="quantity" class="form-label">Quantity</label>
      <input type="number" class="form-control" id="quantity" placeholder="Enter quantity" required>
    </div>
    <div class="col-md-4 mb-3">
      <label for="expiryDate" class="form-label">Expiry Date</label>
      <input type="date" class="form-control" id="expiryDate" required>
    </div>
  </div>
  <button type="submit" class="btn btn-primary">Add Medicine</button>
</form>
<script src="/js/entityScripts.js"></script>
</body>
